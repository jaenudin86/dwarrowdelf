<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="clr-namespace:Dwarrowdelf.Client">

	<DataTemplate DataType="{x:Type local:ItemObject}">
		<local:ItemInfoControl />
	</DataTemplate>

	<DataTemplate DataType="{x:Type local:Living}">
		<local:LivingInfoControl />
	</DataTemplate>

	<DataTemplate DataType="{x:Type local:BuildingObject}">
		<local:BuildingInfoControl />
	</DataTemplate>
</ResourceDictionary>