<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="MyGame.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
		</sectionGroup>
	</configSections>
	<userSettings>
		<MyGame.Properties.Settings>
			<setting name="DebugClient" serializeAs="String">
				<value>True</value>
			</setting>
			<setting name="DebugServer" serializeAs="String">
				<value>True</value>
			</setting>
		</MyGame.Properties.Settings>
	</userSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
	</startup>

	<system.diagnostics>
		<trace autoflush="true" indentsize="4" />

		<sources>
			<source name="System.Windows.Data" switchName="SourceSwitch" >
				<listeners>
					<add name="myTraceListener" />
				</listeners>
			</source>
		</sources>

		<switches>
			<add name="SourceSwitch" value="All" />
		</switches>

		<sharedListeners>
			<add name="myTraceListener" type="MyGame.MyTraceListener,MyGame.Common" />
		</sharedListeners>
	</system.diagnostics>
</configuration>
